{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WeatherForecastApi": {
    "Client": {
      "TenantId": "TenantId",
      "ClientId": "ClientId",
      "Scopes": [
        "api://ClientId/.default"
      ]
    },
    "Endpoint": "https://WeatherForecastApiEndpoint/",
    "PolicyOptions": {
      "RetryCount": "3",
      "HandledEventCount": "5",
      "BreakDuration": "5"
    }
  },
  "Microsoft": {
    "Graph": {
      "TenantId": "TenantId",
      "AuthenticationUri": "https://login.microsoftonline.com/TenantId",
      "GraphUri": "https://graph.microsoft.com"
    }
  },
  "CachingOptions": {
    "AbsoluteExpirationRelativeToNow": "01:00:00"
  },
  "MsalTokenCacheOptions": {
    "Encrypt": false
  }
}
